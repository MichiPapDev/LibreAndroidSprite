<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#03DAC5</color>
    <color name="colorBackground">#FFFFFF</color>
    <color name="colorOnAccent">#000000</color>
    <color name="colorOnPrimary">#FFFFFF</color>
    <color name="colorPrimary">#4CAF50</color>
    <color name="colorPrimaryDark">#388E3C</color>
    <color name="colorSurface">#FFFFFF</color>
    <color name="colorTextPrimary">#000000</color>
    <color name="colorTextSecondary">#666666</color>
    <string name="app_name">LibreAndroidSprite</string>
    <style name="Theme.LibreAndroidSprite" parent="Theme.Material3.Light.NoActionBar">

        
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/colorOnPrimary</item>

        <item name="colorSecondary">@color/colorAccent</item>
        <item name="colorOnSecondary">@color/colorOnAccent</item>

        
        <item name="colorSurface">@color/colorSurface</item>

        
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
        <item name="android:navigationBarColor">@color/colorBackground</item>

        
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>
</resources>